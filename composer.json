{
    "name": "mesingh/amazon-mws-laravel",
    "description": "Use Amazon's MWS web services with Laravel 5.x. Based on przemekperon/amazon-mws-laravel package and modified to make it compatible with dynamic config values also.",
    "license": "Apache-2.0",
    "keywords": ["API", "Amazon", "PHP", "MWS", "Laravel"],
    "authors": [
        {
            "name": "Mandeep Singh",
            "email": "im@msingh.me"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-curl": "*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-0": {
            "MeSingh\\AmazonMws": "src/"
        }
    },
    "minimum-stability": "stable"
}
